    "manifest_version": 3,
    "name": "GitHubRepos",
    "description": "Helps you list all your GitHub repos.",
    "version": "1.0",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "imgs/icon.png"
        }
    }
}